<!DOCTYPE html>
<html>
  <head>
    <title>RailApp</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= content_for?(:title) ? yield(:title) : "Basecamp" %></title>
    <meta name="description" content="<%= content_for?(:description) ? yield(:description) : "Basecamp" %>">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body class="fix-header">
  <div id="wrapper">
  <% if session[:user_id ]%>
  <nav class="navbar navbar-default navbar-static-top m-b-0" style="padding: 0px 0px !important">
            <div class="navbar-header">
                <div class="top-left-part">
                    <!-- Logo -->
                    <a class="logo" href="dashboard.html">
                        <!-- Logo icon image, you can use font-icon also -->
                        <b>
                            <!--This is dark logo icon-->
                            <img src="../plugins/images/admin-logo.png" alt="home" class="dark-logo" />
                            <!--This is light logo icon-->
                            <img src="../plugins/images/admin-logo-dark.png" alt="home" class="light-logo" />
                        </b>
                        <!-- Logo text image you can use text also -->
                        <span class="hidden-xs">
                            <!--This is dark logo text-->
                            <img src="../plugins/images/admin-text.png" alt="home" class="dark-logo" />
                            <!--This is light logo text-->
                            <img src="../plugins/images/admin-text-dark.png" alt="home" class="light-logo" />
                        </span> 
                    </a>
                </div>
                <!-- /Logo -->
                <ul class="nav navbar-top-links navbar-right pull-right">
                    <li>
                        <a class="nav-toggler open-close waves-effect waves-light hidden-md hidden-lg" href="javascript:void(0)"><i class="fa fa-bars"></i></a>
                    </li>
                    <li>
                        <form role="search" class="app-search hidden-sm hidden-xs m-r-10">
                            <input type="text" placeholder="Search..." class="form-control"> 
                            <a href="">
                                <i class="fa fa-search"></i>
                            </a> 
                        </form>
                    </li>
                    <li>
                        <a class="profile-pic" href="#"> <img style="height:50px; width:50px; border-radius:100% " src="data:image/jpeg;base64," alt="user-img" width="36" class="img-circle"><b class="hidden-xs"><%= User.find(session[:user_id]).username %></b></a>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-header -->
            <!-- /.navbar-top-links -->
            <!-- /.navbar-static-side -->
        </nav>
        <!-- End Top Navigation -->
        <!-- ============================================================== -->
        <!-- Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <div class="navbar-default sidebar" role="navigation">
            <div class="sidebar-nav slimscrollsidebar">
                <div class="sidebar-head">
                    <h3><span class="fa-fw open-close"><i class="ti-close ti-menu"></i></span> <span class="hide-menu">Navigation</span></h3>
                </div>
                <ul class="nav" id="side-menu">
                    <li style="padding: 70px 0 0;">
                        <a href="<%= dashboard_projects_path %>" class="waves-effect"><i class="fa fa-clock-o fa-fw" aria-hidden="true"></i>Dashboard</a>
                    </li>
                    <% 
                    user = User.find(session[:user_id])
                    if user.admin_access == 1 %>
                    <li>
                        <a href="<%= admin_users_path %>" class="waves-effect"><i class="fa fa-sign-in fa-fw" aria-hidden="true"></i>Control Panel</a>
                    </li>
                    <% end %>
                    <li>
                        <a href="<%= profile_users_path %>" class="waves-effect"><i class="fa fa-user fa-fw" aria-hidden="true"></i>Profile</a>
                    </li>
                    <li>
                        <a href="<%= projects_path %>" class="waves-effect"><i class="fa fa-table fa-fw" aria-hidden="true"></i>Projects</a>
                    </li>
                    <li>
                        <a href="<%= logout_path %>" class="waves-effect"><i class="fa fa-external-link fa-fw" aria-hidden="true"></i>Logout</a>
                    </li>

                </ul>
            </div>
            
        </div>
        <!-- ============================================================== -->
        <!-- End Left Sidebar -->
    <% end %>
    <%= yield %>
  </div>
  
  <%= javascript_pack_tag 'sidebar-nav' %>
  <%= javascript_pack_tag 'custom' %>
  <%= javascript_pack_tag 'dashboard1' %>
  </body>
</html>
